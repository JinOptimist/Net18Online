@model WebPortalEverthing.Models.DND.ClassCreationViewModel

@{
    Layout = "~/Views/Shared/_LayoutDND.cshtml";
}

<form method="post">
    <div>
        @Html.TextBoxFor(x => x.Name, new { placeholder = "Введи название" })
    </div>
    <div>
        @Html.TextBoxFor(x => x.Url, new { placeholder = "Введи путь к картинке" })
    </div>
    <button type="submit">Создать</button>
</form>