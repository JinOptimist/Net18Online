@using WebPortalEverthing.Models.LoadTesting.Auth
@model LoginLoadUserViewModel

<div class="login-form">

    <form method="post">
        <div>
            @Html.TextBoxFor(x => x.Login, new { placeholder = "login" })
        </div>
        <div>
            @Html.TextBoxFor(x => x.Password, new { @type = "password", placeholder = "password" })
        </div>
        <div>
            <button type="submit">Sign in</button>
        </div>
    </form>
</div>

