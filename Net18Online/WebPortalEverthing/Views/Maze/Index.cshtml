@using WebPortalEverthing.Models.Maze
@model MazeViewModel

@section Styles {
    <link href="~/css/maze/maze.css" rel="stylesheet" />
}

<div>
    Лабиринт других размером можно создать <a href="/Maze/CustomMaze"> тут </a>
</div>
<div>
    Я лабиринт
    <div class="maze">
        @foreach (var row in Model.Rows)
        {
            <div class="row">
                @foreach (var cellUrl in row.CellsUrls)
                {
                    <span class="cell"><img src="@cellUrl" alt="cell" /></span>
                }
            </div>
        }
    </div>
</div>

